#!/bin/bash
#
#Script, mis otsib var kataloogis kasutajale k채ttesaadavad .log l천puga failid
#
find /var/ -name "*.log" 2>>/dev/null -exec wc -l {} \; | sort -n -r #leiab /var/ kaustast *.log failid ja vead saadab /dev/null-i -exec ja siis wordcount -l n채itab mitu faili kokku on | sorteerib kahanevalt 
#
#L천pp 
